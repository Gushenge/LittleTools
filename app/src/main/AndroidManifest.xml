<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gushenge.testdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <!-- 全面屏适配 -->
        <meta-data
            android:name="android.max_aspect"
            android:value="2.1" />

        <activity android:name="com.gushenge.testdemo.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.gushenge.testdemo.SwipeLeftOrRight.SwipeActivity" />
        <activity android:name="com.gushenge.testdemo.EmulatorCheck.EmulatorActivity" />
        <activity android:name="com.gushenge.testdemo.TaobaoNews.NewsActivity"></activity>
    </application>

</manifest>